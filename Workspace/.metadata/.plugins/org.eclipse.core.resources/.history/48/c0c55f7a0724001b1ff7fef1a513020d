package abstraction;

public class rectangle {
	private double radius;
	
	(){}
	Circle(String color, double radius ){
		super(color) ;
		this.radius = radius ;
		
	}
	
	@Override
	public double area() {
		return 3.14 * radius * radius ;
	}
	@Override
	public double perimeter() {
		return 2 * 3.14 * radius ;
		
	}

}
